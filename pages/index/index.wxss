.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}

.index_cate {
  display: flex;
  height: 202rpx;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate image {
  width: 100%;
}

.index_floor .floor_title {
  margin-top: 10rpx;
}
.index_floor .floor_title image {
  width: 100%;
}
.index_floor .floor_list {
  height: 416rpx;
  margin-bottom: 10rpx;
}
.index_floor .floor_list navigator {
  float: left;
  width: 33.33%;
  height: 27.72711207vw;
  border-right: 10rpx solid #fff;
}
.index_floor .floor_list navigator:first-child {
  height: 56vw;
}
.index_floor .floor_list navigator:nth-child(2),
.index_floor .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_list navigator:nth-child(4),
.index_floor .floor_list navigator:nth-child(5) {
  border-bottom: 10rpx solid transparent;
}
.index_floor .floor_list image {
  width: 100%;
  height: 100%;
}